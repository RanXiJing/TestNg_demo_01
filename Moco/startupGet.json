[
  {
    "description":"模拟一个没有参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "text":"This is a get request with no parameters"
    }
  },
  {
    "description":"模拟一个带参数的get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries":{
        "name":"ranxi",
        "age":"21"
      }
    },
    "response":{
      "text":"My ranxi is back!!!"
    }
  }
]