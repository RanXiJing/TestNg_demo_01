[
  {
    "description":"这是一个带header信息的post请求",
    "request":{
      "uri":"/post/with/headers",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"ranxi",
        "sex":"woman"
      }
    },
    "response":{
      "status":200,
      "json":{
        "name":"wangwenjing",
        "sex":"woman"
      }
    }
  }
]